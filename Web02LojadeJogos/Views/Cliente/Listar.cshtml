@model Web02LojadeJogos.Models.Cliente
@{
    ViewBag.Title = "Listar";
}

<fieldset style="margin-top: 20px">
    <legend>Cliente Recém-Cadastrado</legend>
    
    <dl class="dl-horizontal">
        <!-- Output Nome -->
        <dt>@Html.DisplayNameFor(model => model.CliNome)</dt>
        <dd>@Html.DisplayFor(model => model.CliNome)</dd>

        <!-- Output Cpf -->
        <dt>@Html.DisplayNameFor(model => model.CliCpf)</dt>
        <dd>@Html.DisplayFor(model => model.CliCpf)</dd>

        <!-- Output Nasc -->
        <dt>@Html.DisplayNameFor(model => model.CliNasc)</dt>
        <dd>@Html.DisplayFor(model => model.CliNasc)</dd>

        <!-- Output Email -->
        <dt>@Html.DisplayNameFor(model => model.CliEmail)</dt>
        <dd>@Html.DisplayFor(model => model.CliEmail)</dd>

        <!-- Output Celular -->
        <dt>@Html.DisplayNameFor(model => model.CliCelular)</dt>
        <dd>@Html.DisplayFor(model => model.CliCelular)</dd>

        <!-- Output Endereco -->
        <dt>@Html.DisplayNameFor(model => model.CliEndereco)</dt>
        <dd>@Html.DisplayFor(model => model.CliEndereco)</dd>

    </dl>
    @Html.ActionLink("Voltar", "Index", "Cliente")
</fieldset>
